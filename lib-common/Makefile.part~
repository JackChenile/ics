# This file will be included by the Makefile under the project directory.

###replace ".a" with ".o" ###
FLOAT_OBJ := $(FLOAT:.a=.o)

### make target ###
$(FLOAT): $(FLOAT_OBJ)
	ar r $(FLOAT) $(FLOAT_OBJ)

# TODO: write a rule for generating $(FLOAT_OBJ)

CFLAGS_EXTRA := -m32 -fno-builtin

$(FLOAT_OBJ): FLOAT.c
	$(CC) $(CFLAGS) $(CFLAGS_EXTRA) -o $(FLOAT_OBJ) FLOAT.c
