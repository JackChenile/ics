# This file will be included by the Makefile under the project directory.

###replace ".a" with ".o" ###
FLOAT_OBJ := $(FLOAT:.a=.o)

#FLOAT_BIN := FLOAT
### make target ###
$(FLOAT): $(FLOAT_OBJ)
	ar r $(FLOAT) $(FLOAT_OBJ)

# TODO: write a rule for generating $(FLOAT_OBJ)

FLOAT_CFLAGS_EXTRA := -c -m32 -fno-builtin

$(FLOAT_OBJ): 
	$(CC) $(FLOAT_CFLAGS_EXTRA) /lib-common/FLOAT.c -I $(LIB_COMMON_DIR) -o obj/$(LIB_COMMON_DIR)/FLOAT.o
